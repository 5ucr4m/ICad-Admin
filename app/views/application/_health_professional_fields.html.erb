<div class="nested-fields">
  <div class="form-row">
    <%= f.input :full_name, wrapper_html: {class: 'col-lg-6 col-sm-12'}, input_html: {disabled: show?(f.object.persisted?)} %>
    <%= f.input :federal_registry, wrapper_html: {class: 'col-lg-3 col-sm-12'}, input_html: {class: 'cpf', disabled: show?(f.object.persisted?)} %>
    <%= f.input :cns_code, wrapper_html: {class: 'col-lg-3 col-sm-12'}, input_html: {disabled: show?(f.object.persisted?)} %>
    <%= f.association :cbo_code,
                      collection: @cbo_selected ? [@cbo_selected] : [],
                      selected: @cbo_selected,
                      label_method: :name_formatted,
                      wrapper_html: {class: 'col-lg-6 col-sm-12'}, input_html: {class: 'cbo-types', disabled: show?(f.object.persisted?)} %>
    <%= f.association :professional_team, collection: ProfessionalTeam.all,
                      label_method: :name_formatted, wrapper_html: {class: 'col-lg-6 col-sm-12'}, input_html: {class: 'select2', disabled: show?(f.object.persisted?)} %>
  </div>
</div>
