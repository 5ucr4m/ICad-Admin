<%= link_to f, class: 'btn btn-info btn-icon mr-1', role: 'button' do %>
  <i class="fas fa-search"></i>
<% end %>
<%= link_to send("edit_#{f.model_name.singular}_path", f),
            class: 'btn btn-primary btn-icon mr-1', role: 'button' do %>
  <i class="fas fa-edit"></i>
<% end %>
<%= link_to f, class: 'btn btn-danger btn-icon', method: :delete,
            data: {confirm: "Confirma a remoção do registro de código ##{f.id}?"},
            role: 'button' do %>
  <i class="fas fa-trash"></i>
<% end %>
