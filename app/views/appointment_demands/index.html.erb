<%= render 'application/page_header', model: AppointmentDemand %>
<div class="card">
  <div class="card-status bg-transparent"></div>
  <div class="card-header p-4">
    <div class="col-sm-12 col-lg-12">
      <div class="form-row">
        <div class="col-sm-12 col-lg-6 p-0">
          <%= render 'application/pagy_count', list: @pagy %>
        </div>
        <div class="col-sm-12 col-lg-6 p-0">
          <%= search_form_for @query do |f| %>
          <%= f.search_field :search_cont, class: 'form-control', placeholder: t('helpers.search') %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
  <div class="card-body table-responsive p-0 m-0">
    <table class="table card-table table-vcenter text-nowrap table-hover">
      <thead>
      <tr>
                  <th><%= AppointmentDemand.human_attribute_name :family_member %></th>
                  <th><%= AppointmentDemand.human_attribute_name :health_professional %></th>
                  <th><%= AppointmentDemand.human_attribute_name :manage_medicine %></th>
                  <th><%= AppointmentDemand.human_attribute_name :curative %></th>
                  <th><%= AppointmentDemand.human_attribute_name :appointment_demand %></th>
                  <th><%= AppointmentDemand.human_attribute_name :initial_listening %></th>
                  <th><%= AppointmentDemand.human_attribute_name :examination %></th>
                  <th><%= AppointmentDemand.human_attribute_name :nebulization %></th>
                  <th><%= AppointmentDemand.human_attribute_name :dentistry %></th>
                  <th><%= AppointmentDemand.human_attribute_name :procedures %></th>
                  <th><%= AppointmentDemand.human_attribute_name :vaccine %></th>
                  <th><%= AppointmentDemand.human_attribute_name :company %></th>
                  <th><%= AppointmentDemand.human_attribute_name :discarded_at %></th>
                  <th><%= AppointmentDemand.human_attribute_name :ip %></th>
                <th><%= t('helpers.actions.action') %></th>
      </tr>
      </thead>
      <tbody>
      <% @appointment_demands.each do |appointment_demand| %>
        <tr>
                      <td><%= appointment_demand.family_member_id %></td>
                      <td><%= appointment_demand.health_professional_id %></td>
                      <td><%= appointment_demand.manage_medicine %></td>
                      <td><%= appointment_demand.curative %></td>
                      <td><%= appointment_demand.appointment_demand %></td>
                      <td><%= appointment_demand.initial_listening %></td>
                      <td><%= appointment_demand.examination %></td>
                      <td><%= appointment_demand.nebulization %></td>
                      <td><%= appointment_demand.dentistry %></td>
                      <td><%= appointment_demand.procedures %></td>
                      <td><%= appointment_demand.vaccine %></td>
                      <td><%= appointment_demand.company_id %></td>
                      <td><%= appointment_demand.discarded_at %></td>
                      <td><%= appointment_demand.ip %></td>
                    <td class="text-center">
            <%= render 'application/table_links', f: appointment_demand %>
          </td>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>
  <div class="card-footer">
    <%= render 'application/footer_links', url: new_appointment_demand_path, model: AppointmentDemand %>
  </div>
</div>
