          <%= f.text_field :pis_pasep_number, {class: 'form-control', disabled: show?(f.object.persisted?)} %>
        
      </div>
      <div class="form-group col-md-4">
        <%= f.label :unknown_mother_name, {class: 'form-label'} %>
        
          <%= f.check_box :unknown_mother_name, {class: 'form-control', disabled: show?(f.object.persisted?)} %>
        
