<%= form_with(model: @citizen, id: :citizen) do |f| %>
  <% content_for :body do %>
      <div class="form-group col-md-4">
        <%= f.label :registry_id, {class: 'form-label'} %>
        
          <%= f.select :registry_id, options_from_collection_for_select([], :name, :id), {include_blank: 'Selecione'}, {class: 'form-control', disabled: show?(f.object.persisted?)} %>
        
      </div>
      <div class="form-group col-md-4">
        <%= f.label :gender_id, {class: 'form-label'} %>
        
          <%= f.select :gender_id, options_from_collection_for_select([], :name, :id), {include_blank: 'Selecione'}, {class: 'form-control', disabled: show?(f.object.persisted?)} %>
        
      </div>
      <div class="form-group col-md-4">
